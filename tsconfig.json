{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "rootDir": "./",
    "strict": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}